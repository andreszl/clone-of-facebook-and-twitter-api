{
  "name": "clone-of-facebook-and-twitter-api",
  "version": "1.0.0",
  "description": "clone-of-facebook-and-twitter-api",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "start": "set NODE_ENV=production&& ts-node src/index.ts",
    "start:unix": "export NODE_ENV=production&& ts-node src/index.ts",
    "nodemon": "npx nodemon",
    "test:qa": "set NODE_ENV=qa&& npx jest  --runInBand --detectOpenHandles",
    "test": "set NODE_ENV=test&& npx jest --runInBand --detectOpenHandles",
    "test:watch": "set NODE_ENV=test&& npx jest --runInBand --detectOpenHandles --watch",
    "build": "tsc",
    "start:prod": "pm2 node ./build/index.js",
    "lab": "ts-node lab.ts",
    "watch:debug": "nodemon --inspect=5858 -e ts,tsx --exec node -r ts-node/register ./index.ts",
    "migrate": "sh ./scripts/migrate.sh",
    "migrate:seed": "sh ./scripts/migrate_seed.sh",
    "seeders": "sh ./scripts/seeders.sh",
    "pre-commit": "npm run lint && npm run test",
    "pre-push": "npm run pre-commit"
  },
  "keywords": [
    "Express",
    "Typescript",
    "Node",
    "MongoDB",
    "GrahpQL",
    "Apollo Server",
    "ReactJS",
    "API REST",
    "Eslint"
  ],
  "author": "andreszl",
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^4.1.11",
    "@types/graphql": "^14.5.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^25.2.1",
    "@types/jquery": "^3.5.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/mongodb": "^3.6.20",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^13.5.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.48.7",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.10",
    "@commitlint/cli": "^12.1.2",
    "@commitlint/config-conventional": "^12.1.2"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.14",
    "apollo-server-express": "^2.9.16",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "config": "^3.2.5",
    "consolidate": "^0.15.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "history": "^4.9.0",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.5.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.3",
    "nodemon": "^2.0.7",
    "repl.history": "^0.1.4",
    "request": "^2.88.2",
    "supertest": "^4.0.2",
    "swig": "^1.4.2",
    "ts-jest": "^25.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
